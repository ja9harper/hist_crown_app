


  <div id='map'></div>



  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Address</th>
        <th>Image</th>
        <th>History</th>
        <th>Latlng</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @places.each do |place| %>
        <tr>
          <td><%= place.name %></td>
          <td><%= place.address %></td>
          <td><%= place.image %></td>
          <td><%= place.history %></td>
          <td><%= place.latlng %></td>
          <td><%= link_to 'Show', place %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <br>
</div>


